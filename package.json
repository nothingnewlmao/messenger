{
  "name": "project-hw-1",
  "version": "1.0.0",
  "description": "messenger for project 1",
  "scripts": {
    "dev:parcel": "parcel src/pages/index.html src/pages/**/index.html --no-cache",
    "dev:ts": "tsc --watch --noEmit",
    "dev": "run-p -l dev:**",
    "lint:ts": "eslint .",
    "lint:css": "stylelint **/*.css **/*.scss",
    "lint": "run-p -l lint:**",
    "start": "rm -rf dist && parcel build src/pages/index.html src/pages/**/index.html --public-url ../ && npm run server",
    "build": "rm -rf dist && parcel build src/pages/index.html src/pages/**/index.html --public-url ../",
    "server": "node app.js"
  },
  "keywords": [],
  "author": "Valentine",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-xo": "^0.37.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "precss": "^4.0.0",
    "sass": "^1.35.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "material-design-icons": "^3.0.1",
    "material-icons": "^1.0.0",
    "parcel-plugin-asset-copier": "^1.1.0"
